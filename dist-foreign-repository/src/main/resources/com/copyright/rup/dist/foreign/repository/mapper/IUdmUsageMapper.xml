            #{reportedPubType},
            #{pubFormat},
            #{article},
            #{language},
            #{companyId},
            #{companyName},
            #{detailLicenseeClassId},
            #{surveyRespondent},
            #{ipAddress},
            #{surveyCountry},
            #{usageDate},
            #{surveyStartDate},
            #{surveyEndDate},
            #{annualMultiplier},
            #{statisticalMultiplier},
            #{annualizedCopies},
            #{typeOfUse},
            #{reportedTypeOfUse},
            #{quantity},
            #{actionReasonId},
            #{ineligibleReasonId},
            <include refid="StoredEntityMapper.insert"/>
        )
    </insert>

    <select id="isOriginalDetailIdExist" parameterType="string" resultType="boolean">
        select exists (
            select 1
            from ${schema}.df_udm_usage
            where original_detail_id = #{originalDetailId}
