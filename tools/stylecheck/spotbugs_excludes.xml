<FindBugsFilter>
    <Match>
        <!--Application is dealing with Date objects so EXPOSE checks are redundant so far. Serialization checks isn't neccessary-->
        <Bug pattern="EI_EXPOSE_REP,EI_EXPOSE_REP2,SE_BAD_FIELD,SE_NO_SERIALVERSIONID, NP_PARAMETER_MUST_BE_NONNULL_BUT_MARKED_AS_NULLABLE"/>
    </Match>
    <Match>
        <!--Field can be initialized via postConstruct annotation or via spring or by calling another method. So, this check is not required-->
        <Package name="~com\.copyright\.rup\.dist\.foreign\.[service|ui].*"/>
        <Bug pattern="UWF_FIELD_NOT_INITIALIZED_IN_CONSTRUCTOR"/>
    </Match>
    <Match>
        <!--For Vaadin based applications it is common to have inner classes for listeners. The are not serializable and shouldn't be-->
        <Package name="~com\.copyright\.rup\.dist\.foreign\.ui.*"/>
        <Bug pattern="SE_INNER_CLASS"/>
    </Match>
    <Match>
        <!--Additional exclusion for tests-->
        <Class name="~.*\.*Test.*" />
        <Bug pattern="URF_UNREAD_PUBLIC_OR_PROTECTED_FIELD,UWF_FIELD_NOT_INITIALIZED_IN_CONSTRUCTOR,SIC_INNER_SHOULD_BE_STATIC_ANON,SE_BAD_FIELD_INNER_CLASS,BC_UNCONFIRMED_CAST,BC_UNCONFIRMED_CAST_OF_RETURN_VALUE"/>
    </Match>
    <Match>
        <!--Autogenerated swagger files-->
        <Package name="([a-zA-Z0-9.]){0,}.gen.([a-zA-Z0-9.]){0,}"/>
    </Match>
    <Match>
        <!--TODO fix the classes and remove the rule-->
        <Class name="com.copyright.rup.dist.foreign.integration.rms.impl.RmsRightsCacheService"/>
        <Bug pattern="MC_OVERRIDABLE_METHOD_CALL_IN_CONSTRUCTOR"/>
    </Match>
</FindBugsFilter>
